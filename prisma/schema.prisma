// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      =  "file:/dev.db"
}

model Post {
  id Int @id @default(autoincrement())
  container String
  title String
  createdAt DateTime
  subject String
}



model User {
  id       String @id @default(uuid())
  email    String   @unique
  password String
  infos String?
}